<div class="form-group">
  <%= f.label :title %><br>
  <%= f.text_field :title, class: 'form-control', :placeholder => "What is the title of the song?" %>
</div>

<div class="form-group">
  <%= f.label :artist %><br>
  <%= f.text_field :artist, class: 'form-control', :placeholder => "Include all artists and features" %>
</div>

<div class="form-group">
  <%= f.label :album %><br>
  <%= f.text_field :album, class: 'form-control', :placeholder => "What is the title of the album/mixtape?" %>
</div>

<div class="form-group">
  <%= f.label :url %><br>
  <%= f.text_field :url, class: 'form-control', :placeholder => "Enter a YouTube URL or a spotify embed URL" %>
</div>

<div class="form-group">
  <%= f.label :image %><br>
  <%= f.text_field :image, class: 'form-control', :placeholder => "PICS" %>
</div>

<div class="form-group">
  <%= f.label :description %><br>
  <%= f.text_area :description, class: 'form-control', :placeholder => "Describe this song.\nWas this song a single for an album?\nDoes this song have any cultural significance?\nIs there anything notable about the production?\netc." %>
</div>

<div class="form-group">
  <%= f.submit class: 'btn btn-primary' %>
</div>

